{
  "custom": {},
  "params": {
    "Options": "value",
    "SelectedValue": null,
    "Text": "Time"
  },
  "propConfig": {
    "params.Options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.SelectedValue": {
      "binding": {
        "config": {
          "bidirectional": true,
          "path": "/root/Dropdown.props.value"
        },
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50,
      "width": 175
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "50px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_HeaderText"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
          "basis": "50px"
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.Options"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Text/Add_InfoText"
          },
          "placeholder": {
            "icon": {
              "color": "#0d80e3"
            },
            "text": "Select"
          },
          "style": {
            "classes": "Text/Add_InfoText"
          },
          "value": null
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}