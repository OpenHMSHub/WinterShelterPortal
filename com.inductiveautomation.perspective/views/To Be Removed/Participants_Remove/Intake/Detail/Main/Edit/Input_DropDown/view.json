{
  "custom": {},
  "params": {
    "options": {},
    "selection": -1
  },
  "propConfig": {
    "params.options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selection": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 168
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
          "basis": "36px"
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.options"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.selection"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Text/Add_InfoText"
          },
          "style": {
            "classes": "Text/Add_InfoText"
          }
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "justify": "center"
    },
    "type": "ia.container.flex"
  }
}