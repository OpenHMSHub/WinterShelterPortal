          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\timport traceback\n\ttry:\n\t\tproject \u003d \"RITI\"\n\t\t#Declare variables\n\t\t\n\t\tuser_name \u003d self.session.props.auth.user.userName\n\t\thuman_id \u003d self.view.params.human_id\n\t\treferral_provider_id \u003d self.view.params.referral_provider_id\n\t\treferral_status_id \u003d self.view.params.referral_status_id\n\t\treferral_type_id \u003d self.view.params.referral_type_id\n\t\t\n\t\t## Check if parameters are filled in correctly \n\t\tif None in [human_id]:\n\t\t\tvalid \u003d False\n\t\t\tsystem.perspective.print(\u0027Null Error\u0027)\n\t\telif int(human_id) \u003c 0:\n\t\t\tvalid \u003d False\n\t\t\tsystem.perspective.print(\u0027Human ID Error\u0027)\n\t\telse:\n\t\t\tvalid \u003d True\n\t\t\tsystem.perspective.print(\u0027Data Valid\u0027)\n\t\t### Create transaction\n\t\ttransactionFailed \u003d False\n\t\ttxId \u003d system.db.beginTransaction(timeout\u003d5000)\n\t\ttry:\n\t\t\t#Insert the new participant data\n\t\t\tpath \u003d \"Participants/Registration/InsertCopyParticipant\"\n\t\t\tparameters \u003d {\"human_id\":human_id}\n\t\t\tparticipant_id \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\t\t#\n\t\t\t#Insert the referral\n\t\t\ttry:\n\t\t\t\tif int(referral_provider_id) \u003e -1 and int(referral_status_id \u003e -1) and int(referral_type_id \u003e -1):\n\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailInsert\"\n\t\t\t\t\tparameters \u003d {\"participant_id\":participant_id,\n\t\t\t\t\t\t\t\t\"provider_id\":referral_provider_id,\n\t\t\t\t\t\t\t\t\"status_id\":referral_status_id,\n\t\t\t\t\t\t\t\t\"type_id\":referral_type_id}\n\t\t\t\t\treferral \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\t\t\t\t#Insert the first entry inotreferral log\n\t\t\t\t\tstatus_id \u003d referral_status_id\n\t\t\t\t\ttime_created \u003d system.date.now()\n\t\t\t\t\tcomment \u003d \"Automatic referral log entry on new participant addition\"\n\t\t\t\t\treferral_id \u003d referral\n\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailNoteInsert\"\n\t\t\t\t\tparameters \u003d {\"comment\":comment,\n\t\t\t\t\t\t\t\t\"referral_id\":referral_id,\n\t\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\t\"type_id\":referral_type_id,\n\t\t\t\t\t\t\t\t\"time_created\":time_created,\n\t\t\t\t\t\t\t\t\"user_name\":user_name}\n\t\t\t\t\tRefresh \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\t\texcept:\n\t\t\t\tpass\n\t\t\t#\n\t\t\t#Enroll in the Hope University and Guest House programs\n\t\t\t#Hope University is program #29\n\t\t\tpath \u003d \"Participants/Enrollments/ParticipantAddSelectedEnrollments\"\n\t\t\tparameters \u003d {\"participantId\":participant_id,\"programId\":29}\n\t\t\tenrollment \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\t\t#Guest House is program #47\n\t\t\tpath \u003d \"Participants/Enrollments/ParticipantAddSelectedEnrollments\"\n\t\t\tparameters \u003d {\"participantId\":participant_id,\"programId\":47}\n\t\t\tenrollment \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\t\t#Campus Store is program #24\n\t\t\tpath \u003d \"Participants/Enrollments/ParticipantAddSelectedEnrollments\"\n\t\t\tparameters \u003d {\"participantId\":participant_id,\"programId\":24}\n\t\t\tenrollment \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, tx\u003dtxId, getKey\u003dTrue)\n\t\texcept:\n\t\t\ttransactionFailed \u003d True\n\t\t### Check if an error occured rollback the transaction\n\t\tif transactionFailed:\n\t\t\tsystem.db.rollbackTransaction(txId)\n\t\t\tself.parent.parent.getChild(\"FlexContainer\").getChild(\"Label\").showMessage(\"Something went wrong while saving data. Please try again.\")\n\t\t\tsystem.perspective.print(\u0027Transaction Failed\u0027)\n\t\t\tRefresh \u003d 0\n\t\t### Otherwise commit \n\t\telse:\n\t\t\t system.db.commitTransaction(txId)\n\t\t### Always close the transaction\t\n\t\tsystem.db.closeTransaction(txId)\t\n\t\tif not transactionFailed:\n\t\t\t#Close the dock\n\t\t\tsystem.perspective.sendMessage(\"ResetSearch\", payload\u003d{}, scope\u003d\"session\")\n\t\t\tsystem.perspective.sendMessage(\"ResetRegistration\", payload\u003d{}, scope\u003d\"session\")\n\t\t\tsystem.perspective.closeDock(\u0027RegisterParticipant\u0027)\n\t\t\tsystem.perspective.closePopup(\u0027pop_participants_copy_human\u0027)\n\t\t\t#Go to the detail view\n\t\t\ttab_id \u003d 10\n\t\t\tsystem.perspective.navigate(\"Participants/Detail/\" + str(participant_id) + \"/\" + str(tab_id))\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants PopupCopyHuman buttonYes\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)"
                  },
