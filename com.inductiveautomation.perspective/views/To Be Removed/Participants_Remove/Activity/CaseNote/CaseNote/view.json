  "propConfig": {
    "custom.Data": {
      "binding": {
        "config": {
          "parameters": {
            "CaseNoteID": "{view.params.ID}"
          },
          "queryPath": "Participants/Activities/SelectSingleCaseNote",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value\n\telse:\n\t\treturn [{\n\t  \"HMIS\": \"\",\n\t  \"Note\": \"\",\n\t  \"Services\": \"\"\n\t}]",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.Date": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "binding": {
        "config": {
          "path": "view.props.defaultSize.height"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 220
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Date": {
            "binding": {
              "config": {
                "expression": "{view.params.Date}"
              },
              "transforms": [
                {
                  "expression": "dateFormat({value},\"MMM. d, YYYY\") +\" at \"+ dateFormat({value},\"hh:mm a\")",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
