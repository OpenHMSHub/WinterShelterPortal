        "props": {
          "params": {
            "Icon": "material/description",
            "Title": "Case Note"
          },
          "path": "Participants/Activity/ActivityHeader"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_1"
        },
        "position": {
          "basis": "64px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.HMIS": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].HMIS"
              },
              "type": "property"
