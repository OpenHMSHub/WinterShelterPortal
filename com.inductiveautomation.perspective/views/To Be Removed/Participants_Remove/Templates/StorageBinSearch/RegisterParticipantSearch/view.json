        "propConfig": {
          "custom.sql_data": {
            "binding": {
              "config": {
                "expression": "if(hasChanged({view.params.first_name}),true,\r\nif(hasChanged({view.params.middle_name}),true,\r\nif(hasChanged({view.params.last_name}),true,\r\nif(hasChanged({view.params.nick_name}),true,\r\nif(hasChanged({view.params.race_id}),true,\r\nif(hasChanged({view.params.dob}),true,\r\nif(hasChanged({view.params.ssn}),true,\r\nif(hasChanged({view.params.dob_year}),true,\r\nif(hasChanged({view.params.dob_month}),true,\r\nif(hasChanged({view.params.dob_day}),true,\r\nfalse))))))))))"
              },
              "transforms": [
                {
                  "code": "\t#system.perspective.print(\u0027Search Value Change Detected\u0027)\n\t#Assign variables from parameters\n\tfirst_name \u003d self.view.params.first_name\n\tmiddle_name \u003d self.view.params.middle_name\n\tlast_name \u003d self.view.params.last_name\n\tnick_name \u003d self.view.params.nick_name\n\trace_id \u003d self.view.params.race_id\n\tssn \u003d self.view.params.ssn\n\tdob_year \u003d self.view.params.dob_year\n\tdob_month \u003d self.view.params.dob_month\n\tdob_day \u003d self.view.params.dob_day\n\t#system.perspective.print(\u0027year:\u0027 + str(dob_year) + \u0027, month:\u0027 + str(dob_month) + \u0027, day:\u0027 + str(dob_day))\n\t#\n\t#Only run the query if there is search criteria\n\tif (\n\t\tfirst_name !\u003d None or middle_name !\u003d None or last_name !\u003d None or nick_name !\u003d None\n\t\tor race_id !\u003d None or ssn !\u003d None\n\t\tor dob_year !\u003d None or dob_month !\u003d None or dob_day !\u003d None\n\t\t):\n\t\t#system.perspective.print(\u0027Data Requested\u0027)\n\t\tproject \u003d \u0027RITI\u0027\n\t\tpath \u003d \"Participants/Registration/FindMatches\"\n\t\tparameters \u003d {\"first_name\":first_name,\n\t\t\t\t\t\"middle_name\":middle_name,\n\t\t\t\t\t\"last_name\":last_name,\n\t\t\t\t\t\"nick_name\":nick_name,\n\t\t\t\t\t\"race_id\":race_id,\n\t\t\t\t\t\"dob_year\":dob_year,\n\t\t\t\t\t\"dob_month\":dob_month,\n\t\t\t\t\t\"dob_day\":dob_day,\n\t\t\t\t\t\"ssn\":ssn}\n\t\tsearch_results \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\t\n\t\t#return search_results\n\t\t#table_data is the data object to return\n\t\ttable_data \u003d []\n\t\n\t\t#Iterate over the results of the query\n\t\tfor row in range(search_results.getRowCount()):\n\t\t\trow_struct \u003d {}\n\t\t\trow_data \u003d {}\n\t\t\tstyle \u003d {}\t\n\t\t\t#Map query results into correct attributes\n\t\t\trow_data[\u0027human_id\u0027] \u003d search_results.getValueAt(row, \"human_id\")\n\t\t\trow_data[\u0027participant_id\u0027] \u003d search_results.getValueAt(row, \"participant_id\")\n\t\t\tif search_results.getValueAt(row, \"participant_id\") !\u003d None:\n\t\t\t\tif search_results.getValueAt(row, \"participant_retired\") !\u003d None:\n\t\t\t\t\trow_data[\u0027participant\u0027] \u003d 2\n\t\t\t\telse:\n\t\t\t\t\trow_data[\u0027participant\u0027] \u003d 1\n\t\t\telse:\n\t\t\t\trow_data[\u0027participant\u0027] \u003d 0\n\t\t\t\t\n\t\t\tif search_results.getValueAt(row, \"volunteer_id\") !\u003d None:\n\t\t\t\tif search_results.getValueAt(row, \"volunteer_retired\") !\u003d None:\n\t\t\t\t\trow_data[\u0027volunteer\u0027] \u003d 2\n\t\t\t\telse:\n\t\t\t\t\trow_data[\u0027volunteer\u0027] \u003d 1\n\t\t\telse:\n\t\t\t\trow_data[\u0027volunteer\u0027] \u003d 0\n\t\t\t\t\t\n\t\t\tif search_results.getValueAt(row, \"employee_id\") !\u003d None:\n\t\t\t\tif search_results.getValueAt(row, \"employee_retired\") !\u003d None:\n\t\t\t\t\trow_data[\u0027employee\u0027] \u003d 2\n\t\t\t\telse:\n\t\t\t\t\trow_data[\u0027employee\u0027] \u003d 1\n\t\t\telse:\n\t\t\t\trow_data[\u0027employee\u0027] \u003d 0\n\n\t\t\trow_data[\u0027first_name\u0027] \u003d search_results.getValueAt(row, \"first_name\")\n\t\t\trow_data[\u0027middle_name\u0027] \u003d search_results.getValueAt(row, \"middle_name\")\n\t\t\trow_data[\u0027last_name\u0027] \u003d search_results.getValueAt(row, \"last_name\")\n\t\t\trow_data[\u0027nick_name\u0027] \u003d search_results.getValueAt(row, \"nick_name\")\n\t\t\trow_data[\u0027ssn\u0027] \u003d search_results.getValueAt(row, \"ssn\")\n\t\t\trow_data[\u0027dob\u0027] \u003d search_results.getValueAt(row, \"dob\")\n\t\t\trow_data[\u0027race_id\u0027] \u003d search_results.getValueAt(row, \"race_id\")\n\t\t\trow_data[\u0027race\u0027] \u003d search_results.getValueAt(row, \"race\")\n\t\t\t#Set the default style for the row\n\t\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\t#Append the individual row date to the table data object\n\t\t\ttable_data.append(row_struct)\n\t\t\t\t\n\t\treturn table_data\n\telse:\n\t\treturn []",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          },
