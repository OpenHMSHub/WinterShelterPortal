        "transforms": [
          {
            "code": "\timport traceback\n\ttry:\n\t\tproject \u003d \u0027RITI\u0027\n\t\t#Set to blank values as default\n\t\tself.custom.suspensionType \u003d \u0027\u0027\n\t\tself.custom.suspensionDuration \u003d \u0027\u0027\n\t\tself.custom.suspensionTimeBegin \u003d \u0027\u0027\n\t\tself.custom.suspensionTimeEnd \u003d \u0027\u0027\n\t\t\n\t\tif self.params.suspension \u003d\u003d 1 and self.params.row_id \u003e -1:\n\t\t\t#system.perspective.print(self.params.suspension)\n\t\t\trowId \u003d self.params.row_id\n\t\t\tsystem.perspective.print(rowId)\n\t\t\tpath \u003d \"Participants/Incidents/SuspensionDetailSelect\"\n\t\t\tparameters \u003d {\"incident_log_id\":rowId}\n\t\t\tsuspension \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\tsystem.perspective.print(suspension.getRowCount())\n\t\t\tif suspension.getRowCount():\n\t\t\t\t#Map query results into correct attributes\n\t\t\t\tself.custom.suspensionType \u003d suspension.getValueAt(0, \"suspension_type_name\")\n\t\t\t\tself.custom.suspensionDuration \u003d suspension.getValueAt(0, \"suspension_duration\")\n\t\t\t\tself.custom.suspensionTimeBegin \u003d system.date.toMillis(suspension.getValueAt(0, \"suspension_time_begin\"))\n\t\t\t\tself.custom.suspensionTimeEnd \u003d system.date.toMillis(suspension.getValueAt(0, \"suspension_time_end\"))\n\t\t\t\t\n\t\t\t\tif int(self.custom.suspensionDuration):\n\t\t\t\t\tduration \u003d int(self.custom.suspensionDuration)\n\t\t\t\telse:\n\t\t\t\t\tduration \u003d 0\n\t\t\t\tif duration \u003d\u003d 1:\n\t\t\t\t\tduration_name \u003d \"1 Day\"\n\t\t\t\telif duration \u003d\u003d 7:\n\t\t\t\t\tduration_name \u003d \"1 Week\"\n\t\t\t\telif duration \u003d\u003d 14:\n\t\t\t\t\tduration_name \u003d \"2 Weeks\"\n\t\t\t\telif duration \u003d\u003d 30:\n\t\t\t\t\tduration_name \u003d \"30 Days\"\n\t\t\t\telif duration \u003d\u003d 60:\n\t\t\t\t\tduration_name \u003d \"60 Days\"\n\t\t\t\telif duration \u003d\u003d 90:\n\t\t\t\t\tduration_name \u003d \"90 Days\"\n\t\t\t\telif duration \u003d\u003d 365:\n\t\t\t\t\tduration_name \u003d \"1 Year\"\n\t\t\t\telif duration \u003d\u003d 3650:\n\t\t\t\t\tduration_name \u003d \"Indefinite\"\n\t\t\t\telse:\n\t\t\t\t\tduration_name \u003d \"Other\"\n\t\t\t\tself.custom.suspensionDurationName \u003d duration_name\n\n\t\treturn value\n\texcept:\t\t\n\t\tlogger \u003d system.util.getLogger(\"Participants Incidents Dashboard Suspension Query\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)\n\t\t",
            "type": "script"
