              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "label": "Incident Description",
            "placeholder": "Add a description of the incident..."
          },
          "path": "Participants/Templates/120px/top/Input_Text_MultiLine",
          "style": {
            "marginLeft": 10,
            "marginRight": 10
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
