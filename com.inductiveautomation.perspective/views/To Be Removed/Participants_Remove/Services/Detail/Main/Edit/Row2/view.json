{
  "custom": {},
  "params": {
    "comment": "text"
  },
  "propConfig": {
    "params.comment": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 60,
      "width": 1352
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Embed_Comment"
        },
        "position": {
          "basis": "1800px",
          "grow": 1
        },
        "propConfig": {
          "props.params.text": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.comment"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "label": "Comments",
            "placeholder": "Start typing to add a comment..."
          },
          "path": "Participants/Templates/60px/top/Input_Text",
          "style": {
            "classes": "InputCards/InputCard_NoBorder"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Embed_Placeholder"
        },
        "position": {
          "basis": "1px",
          "grow": 1
        },
        "props": {
          "path": "Participants/Templates/Placeholder",
          "style": {
            "classes": "InputCards/Card"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}