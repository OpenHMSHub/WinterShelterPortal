  "root": {
    "children": [
      {
        "events": {
          "component": {
            "onRowClick": {
              "config": {
                "script": "\tselectedColumn \u003d self.props.selection.selectedColumn\n\tnote_id \u003d int(self.props.selection.data[0].note_id)\n\tif selectedColumn \u003d\u003d \"note_id\": \n\t\t#system.perspective.print(\u0027Selected Case Note: \u0027 + str(note_id))\n\t\t#Display the casen notes screen\n\t\tif note_id \u003e 0:\n\t\t\tmessageType \u003d \u0027participant_detail_tab_select\u0027\n\t\t\tpayload \u003d {\"tab_id\":\"Case Notes\"}\n\t\t\tsystem.perspective.sendMessage(messageType, payload)\n\t\t\t#system.perspective.print(\u0027Tab Change Message Sent\u0027)\n\t#system.perspective.print(len(self.props.data))\n\tfor item in range(len(self.props.data)):\n\t\tif item \u003d\u003d event.row:\n\t\t\tself.props.data[item].style.classes \u003d \u0027Table/Row_Select_Detail\u0027\n\t\telse:\n\t\t\tself.props.data[item].style.classes \u003d \u0027Table/Cell_Detail\u0027"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "props.data": {
            "binding": {
              "config": {
                "parameters": {
                  "participant_id": "{view.params.participantID}"
                },
                "queryPath": "Participants/Services/ServicesDetailSelect"
              },
              "transforms": [
                {
                  "code": "\t\"\"\"\n\t\t####THIS SECTION FOR INFORMATION ONLY####\n\t\t#Example Drug Screen Detail data object should look like this:\n\t\t{\n\t\t\t\"value\": {\n\t\t\t  \"row_id\": 0\n\t\t\t  \"employee_id\":0\n\t\t\t  \"employee_name\": \"Susan Jones\"\n\t\t\t  \"enrollment_id\":0\n\t\t\t  \"enrollment_name\": \"Hope University\"\n\t\t\t  \"type_id\":0\n\t\t\t  \"service_id\":0\n\t\t\t  \"service_name\": \"Resume\"\n\t\t\t  \"time_created\": \"07/30/2020\",\n\t\t\t \t }\n\t\t\t\"style\": {\n\t\t\t      \"classes\" \u003d \"Table/Cell_Detail\"\n\t\t\t\t  }\n\t\t\t}\n\t####END INFORMATION####\n\t\t\n\t####BEGIN ACTUAL SCRIPT####\n\t\"\"\"\n\t#table_data is the data object to return\n\ttable_data \u003d []\n\t\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\trow_struct \u003d {}\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\t\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027row_id\u0027] \u003d value.getValueAt(row, \"row_id\")\t\n\t\trow_data[\u0027employee_id\u0027] \u003d value.getValueAt(row, \"employee_id\")\n\t\trow_data[\u0027employee_name\u0027] \u003d value.getValueAt(row, \"employee_name\")\n\t\trow_data[\u0027enrollment_id\u0027] \u003d value.getValueAt(row, \"enrollment_id\")\n\t\trow_data[\u0027enrollment_name\u0027] \u003d value.getValueAt(row, \"enrollment_name\")\n\t\trow_data[\u0027type_id\u0027] \u003d value.getValueAt(row, \"type_id\")\n\t\trow_data[\u0027service_id\u0027] \u003d value.getValueAt(row, \"service_id\")\n\t\trow_data[\u0027service_name\u0027] \u003d value.getValueAt(row, \"service_name\")\n\t\trow_data[\u0027time_created\u0027] \u003d system.date.toMillis(value.getValueAt(row, \"time_created\"))\n\t\trow_data[\u0027HMIS\u0027] \u003d value.getValueAt(row, \"HMIS\")\n\t\trow_data[\u0027quantity\u0027] \u003d value.getValueAt(row, \"quantity\")\n\t\trow_data[\u0027comment\u0027] \u003d value.getValueAt(row, \"comment\")\n\t\t#Query for any associated Case Notes\n\t\tproject \u003d \"RITI\"\n\t\tpath \u003d \"Participants/Services/ServiceCaseNoteSelect\"\n\t\tparameters \u003d {\"service_id\":row_data[\u0027row_id\u0027]}\n\t\tnote_data \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\t\n\t\tif note_data.getRowCount():\n\t\t\trow_data[\u0027note_id\u0027] \u003d note_data.getValueAt(0,\u0027note_id\u0027)\n\t\telse:\n\t\t\trow_data[\u0027note_id\u0027] \u003d 0\n\t\t#Set the default style for the row\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t#Append the individual row date to the table data object\n\t\ttable_data.append(row_struct)\n\t\t\t\t\n\treturn table_data\t",
                  "type": "script"
                }
              ],
              "type": "query"
            }
          }
        },
