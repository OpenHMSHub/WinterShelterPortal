      },
      {
        "custom": {
          "previousSelectedRow": -1
        },
        "events": {
          "component": {
            "onRowClick": {
              "config": {
                "script": "\timport traceback\n\ttry:\n\t\tselectedColumn \u003d self.props.selection.selectedColumn\n\t\tparticipantID \u003d self.props.data.getValueAt(event.row,\u0027participantId\u0027)\n\t\tnote_id \u003d int(self.props.selection.data[0].noteId)\n\t\t#system.perspective.print(selectedColumn)\n\t\t#system.perspective.print(participantID)\n\t\tif selectedColumn \u003d\u003d \"participantId\": \n\t\t\t#Display the detail screen\n\t\t\t#system.perspective.print(\u0027Participant Detail\u0027)\n\t\t\ttab_id \u003d 60\n\t\t\tsystem.perspective.navigate(\"Participants/Detail/\" + str(participantID) + \"/\" + str(tab_id))\n\t\t#elif selectedColumn \u003d\u003d \"Options\":\n\t\telif selectedColumn \u003d\u003d \"rowId\":\n\t\t\tid \u003d \"PreviewParticipant\" \n\t\t\tparams \u003d {\"participantID\":participantID}\n\t\t\tsystem.perspective.openDock(id\u003did,params\u003dparams)\n\n\n\t\telif selectedColumn \u003d\u003d \"noteId\": \n\t\t\t#system.perspective.print(\u0027Selected Case Note: \u0027 + str(note_id))\n\t\t\t#Display the case notes screen\n\t\t\ttab_id \u003d 40\n\t\t\tsystem.perspective.navigate(\"Participants/Detail/\" + str(participantID) + \"/\" + str(tab_id))\n\t\telse:\n\t\t\tpass\n\t\t\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants Dashboard Participants_table OnRowClick\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)"
              },
              "scope": "G",
              "type": "script"
            }
          },
          "system": {
            "onStartup": {
              "config": {
                "script": "\tself.custom.previousSelectedRow \u003d -1"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "custom.sql_data": {
            "access": "PRIVATE",
            "binding": {
              "config": {
                "parameters": {
                  "HMIS": "{session.custom.filters.servicesDashboard.HMIS.where}",
                  "activity_range": "{view.params.activity_range}",
                  "employeeId": "{session.custom.filters.servicesDashboard.employeeId.where}",
                  "firstName": "{session.custom.filters.servicesDashboard.firstName.where}",
                  "lastName": "{session.custom.filters.servicesDashboard.lastName.where}",
                  "middleName": "{session.custom.filters.servicesDashboard.middleName.where}",
                  "nickName": "{session.custom.filters.servicesDashboard.nickName.where}",
                  "programId": "{session.custom.filters.servicesDashboard.programId.where}",
                  "search": "{session.custom.filters.servicesDashboard.Search.where}",
                  "serviceDate": "{session.custom.filters.servicesDashboard.serviceDate.where}",
                  "serviceId": "{session.custom.filters.servicesDashboard.serviceId.where}"
                },
                "queryPath": "Participants/Services/ServicesDashboardSelect",
                "returnFormat": "dataset"
              },
              "type": "query"
            }
          },
          "props.data": {
            "binding": {
              "config": {
                "path": "this.custom.sql_data"
              },
              "transforms": [
                {
                  "code": "\t\"\"\"\n\t\t####THIS SECTION FOR INFORMATION ONLY####\n\t\t#Example Drug Screen Detail data object should look like this:\n\t\t{\n\t\t\t\"value\": {\n\t\t\t  \"row_id\": 0\n\t\t\t  \"employee_id\":0\n\t\t\t  \"employee_name\": \"Susan Jones\"\n\t\t\t  \"enrollment_id\":0\n\t\t\t  \"enrollment_name\": \"Hope University\"\n\t\t\t  \"type_id\":0\n\t\t\t  \"service_id\":0\n\t\t\t  \"service_name\": \"Resume\"\n\t\t\t  \"time_created\": \"07/30/2020\",\n\t\t\t \t }\n\t\t\t\"style\": {\n\t\t\t      \"classes\" \u003d \"Table/Cell_Dashboard\"\n\t\t\t\t  }\n\t\t\t}\n\t####END INFORMATION####\n\t\t\n\t####BEGIN ACTUAL SCRIPT####\n\t\"\"\"\n\t\u0027\u0027\u0027\n\tpyDS \u003d system.dataset.toPyDataSet(value)\n\t\t\n\t#table_data is the data object to return\n\ttable_data \u003d []\n\t\n\t#Iterate over the results of the query\n\tfor row in pyDS:\n\t\trow_struct \u003d {}\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\t\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027rowId\u0027] \u003d row[\"rowId\"]\n\t\trow_data[\u0027participantId\u0027] \u003d row[\"participantId\"]\n\t\trow_data[\u0027name\u0027] \u003d row[\"name\"]\n\t\trow_data[\u0027ssn\u0027] \u003d row[\"ssn\"]\n\t\trow_data[\u0027employeeId\u0027] \u003d row[\"employeeId\"]\n\t\trow_data[\u0027employeeName\u0027] \u003d row[\"employeeName\"]\n\t\trow_data[\u0027programId\u0027] \u003d row[\"programId\"]\n\t\trow_data[\u0027programName\u0027] \u003d row[\"programName\"]\n\t\trow_data[\u0027serviceTypeId\u0027] \u003d row[\"serviceTypeId\"]\n\t\trow_data[\u0027serviceId\u0027] \u003d row[\"serviceId\"]\n\t\trow_data[\u0027serviceName\u0027] \u003d row[\"serviceName\"]\n\t\trow_data[\u0027serviceDate\u0027] \u003d row[\"serviceDate\"]\n\t\trow_data[\u0027HMIS\u0027] \u003d row[\"HMIS\"]\n\t\trow_data[\u0027quantity\u0027] \u003d row[\"quantity\"]\n\t\trow_data[\u0027comment\u0027] \u003d row[\"comment\"]\n\t\t\n\t\t#Query for any associated Case Notes\n\t\tproject \u003d \"RITI\"\n\t\tpath \u003d \"Participants/Services/ServiceCaseNoteSelect\"\n\t\tparameters \u003d {\"service_id\":row_data[\u0027rowId\u0027]}\n\t\tnote_data \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\t\n\t\tif note_data.getRowCount():\n\t\t\trow_data[\u0027note_id\u0027] \u003d note_data.getValueAt(0,\u0027note_id\u0027)\n\t\telse:\n\t\t\trow_data[\u0027note_id\u0027] \u003d 0\n\t\t\t\n\t\t#Set the default style for the row\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Dashboard\"\n\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\n\t\t#Append the individual row date to the table data object\n\t\ttable_data.append(row_struct)\n\t\t\t\t\n\treturn table_data\t\n\t\u0027\u0027\u0027\n\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
