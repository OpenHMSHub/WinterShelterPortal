  "propConfig": {
    "custom.referral_data": {
      "binding": {
        "config": {
          "parameters": {
            "participant_id": "{view.params.participantID}"
          },
          "queryPath": "Participants/Referrals/ReferralsDetailSelect"
        },
        "transforms": [
          {
            "code": "\timport traceback\n\ttry:\n\t\t#query variables\n\t\tproject \u003d \"RITI\"\n\t\t#table_data is the data object to return\n\t\ttable_data \u003d []\n\t\n\t\t#Iterate over the results of the query\n\t\tfor row in range(value.getRowCount()):\n\t\t\trow_struct \u003d {}\n\t\t\trow_data \u003d {}\n\t\t\tstyle \u003d {}\n\t\t\t#Map query results into correct attributes\n\t\t\trow_data[\u0027referral_id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\t\trow_data[\u0027referral_date\u0027] \u003d system.date.toMillis(value.getValueAt(row, \"referral_date\"))\n\t\t\trow_data[\u0027provider_id\u0027] \u003d value.getValueAt(row, \"provider_id\")\n\t\t\trow_data[\u0027provider_name\u0027] \u003d value.getValueAt(row, \"provider_name\")\n\t\t\trow_data[\u0027provider_type_id\u0027] \u003d value.getValueAt(row, \"provider_type_id\")\n\t\t\trow_data[\u0027type_id\u0027] \u003d value.getValueAt(row, \"type_id\")\n\t\t\trow_data[\u0027referral_type\u0027] \u003d value.getValueAt(row, \"referral_type\")\n\t\t\trow_data[\u0027status_id\u0027] \u003d value.getValueAt(row, \"status_id\")\n\t\t\trow_data[\u0027referral_status\u0027] \u003d value.getValueAt(row, \"referral_status\")\n\t\t\trow_data[\u0027program_id\u0027] \u003d value.getValueAt(row, \"program_id\")\n\t\t\trow_data[\u0027program_name\u0027] \u003d value.getValueAt(row, \"program_name\")\n\t\t\t#Set the default style for the row\n\t\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\t#Append the individual row date to the table data object\n\t\t\ttable_data.append(row_struct)\n\t\treturn table_data\n\t\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants ReferralsTableDetail view.custom.referral_data\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.referral_notes": {
      "binding": {
        "config": {
          "path": "view.custom.referral_data"
        },
        "transforms": [
          {
            "code": "\timport traceback\n\ttry:\n\t\treferral_data \u003d value\n\t\tproject \u003d \"RITI\"\n\t\t#table_data is the data object to return\n\t\treferral_notes \u003d []\n\t\t\t\t\n\t\tif len(referral_data):\n\t\t\tfor row in range(len(referral_data)):\n\t\t\t\ttry:\n\t\t\t\t\treferral_id \u003d self.custom.referral_data[row].value.referral_id\n\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralNotesSelect\"\n\t\t\t\t\tparameters \u003d {\"referral_id\":referral_id}\n\t\t\t\t\tcomments \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t\tfor comment in range(comments.getRowCount()):\n\t\t\t\t\t\trow_struct \u003d {}\n\t\t\t\t\t\trow_data \u003d {}\n\t\t\t\t\t\tstyle \u003d {}\t\n\t\t\t\t\t\t#Map query results into correct attributes\n\t\t\t\t\t\trow_data[\u0027referral_id\u0027] \u003d referral_id\n\t\t\t\t\t\trow_data[\u0027note_id\u0027] \u003d comments.getValueAt(comment, \"note_id\")\n\t\t\t\t\t\trow_data[\u0027status_id\u0027] \u003d comments.getValueAt(comment, \"status_id\")\n\t\t\t\t\t\trow_data[\u0027status_name\u0027] \u003d comments.getValueAt(comment, \"status_name\")\n\t\t\t\t\t\trow_data[\u0027type_id\u0027] \u003d comments.getValueAt(comment, \"type_id\")\n\t\t\t\t\t\trow_data[\u0027type_name\u0027] \u003d comments.getValueAt(comment, \"type_name\")\n\t\t\t\t\t\trow_data[\u0027comment\u0027] \u003d comments.getValueAt(comment, \"comment\")\n\t\t\t\t\t\trow_data[\u0027time_created\u0027] \u003d system.date.toMillis(comments.getValueAt(comment, \"time_created\"))\n\t\t\t\t\t\t#Set the default style for the row\n\t\t\t\t\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\t\t\t\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\t\t\t\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\t\t\t\t#Append the individual row date to the table data object\n\t\t\t\t\t\treferral_notes.append(row_struct)\n\t\t\t\texcept:\n\t\t\t\t\tpass\n\t\t\t\t\t\t\t\n\t\t\treturn referral_notes\n\n\t\telse:\n\t\t\treturn referral_notes\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants ReferralsTableDetail view.custom.referral_notes len(referral_data)\u003d\" + str(len(referral_data)))\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "onChange": {
        "enabled": null,
        "script": "\t#Refresh the table data any time the suspensions have changed\n\tself.refreshBinding(\"custom.table_data\")"
      },
      "persistent": true
    },
    "custom.table_data": {
      "binding": {
        "config": {
          "path": "view.custom.referral_data"
        },
        "transforms": [
          {
            "code": "\timport traceback\n\ttry:\n\t\treferral_data \u003d value\n\t\treferral_notes \u003d self.custom.referral_notes\n\t\t#system.perspective.print(str(len(services_data)) + \u0027 services found\u0027)\n\t\tif referral_notes \u003d\u003d None:\n\t\t\treferral_notes \u003d []\n\t\telif len(referral_notes) \u003c 1:\n\t\t\treferral_notes \u003d []\n\t\ttable_data \u003d []\n\t\t#Move the date from the case notes directly to the table\n\t\tif len(referral_data):\n\t\t\tfor row in range(len(referral_data)):\n\t\t\t\trow_struct \u003d {}\n\t\t\t\treferral \u003d {}\n\t\t\t\tnote_list \u003d []\n\t\t\t\tstyle \u003d {}\t\n\t\t\t\t#Map query results into correct attributes\n\t\t\t\t#system.perspective.print(\u0027Table Data Processing Case Note\u0027 + str(notes_data[row].value.note_id))\n\t\t\t\treferral[\u0027referral_id\u0027] \u003d referral_data[row].value.referral_id\n\t\t\t\treferral[\u0027referral_date\u0027] \u003d referral_data[row].value.referral_date\n\t\t\t\treferral[\u0027provider_id\u0027] \u003d referral_data[row].value.provider_id\n\t\t\t\treferral[\u0027provider_name\u0027] \u003d referral_data[row].value.provider_name\n\t\t\t\treferral[\u0027provider_type_id\u0027] \u003d referral_data[row].value.provider_type_id\n\t\t\t\treferral[\u0027type_id\u0027] \u003d referral_data[row].value.type_id\n\t\t\t\treferral[\u0027referral_type\u0027] \u003d referral_data[row].value.referral_type\n\t\t\t\treferral[\u0027status_id\u0027] \u003d referral_data[row].value.status_id\n\t\t\t\treferral[\u0027referral_status\u0027] \u003d referral_data[row].value.referral_status\n\t\t\t\treferral[\u0027program_id\u0027] \u003d referral_data[row].value.program_id\n\t\t\t\treferral[\u0027program_name\u0027] \u003d referral_data[row].value.program_name\n\t\t\t\tRowNumber \u003d 0\n\t\t\t\t#system.perspective.print(str(len(services_data)) + \u0027 services found\u0027)\n\t\t\t\tif len(referral_notes):\n\t\t\t\t\tfor r_row in range(len(referral_notes)):\n\t\t\t\t\t\tnote \u003d {}\n\t\t\t\t\t\t#system.perspective.print(str(note[\u0027note_id\u0027]) + \u0027 \u0027 + str(services_data[s_row].value.note_id))\n\t\t\t\t\t\tif referral[\u0027referral_id\u0027] \u003d\u003d referral_notes[r_row].value.referral_id:\n\t\t\t\t\t\t\t#system.perspective.print(\u0027Table Data Processing Service\u0027 + str(services_data[s_row].value.service_id))\n\t\t\t\t\t\t\tnote[\u0027time_created\u0027] \u003d referral_notes[r_row].value.time_created\n\t\t\t\t\t\t\tnote[\u0027status_id\u0027] \u003d referral_notes[r_row].value.status_id\n\t\t\t\t\t\t\tnote[\u0027status_name\u0027] \u003d referral_notes[r_row].value.status_name\n\t\t\t\t\t\t\tnote[\u0027type_id\u0027] \u003d referral_notes[r_row].value.type_id\n\t\t\t\t\t\t\tnote[\u0027type_name\u0027] \u003d referral_notes[r_row].value.type_name\n\t\t\t\t\t\t\tnote[\u0027comment\u0027] \u003d referral_notes[r_row].value.comment\n\t\t\t\t\t\t\tnote[\u0027row_num\u0027] \u003d RowNumber\n\t\t\t\t\t\t\tnote_list.append(note)\n\t\t\t\t\t\t\tRowNumber +\u003d 1\n\t\t\t\t#Set the default style for the row\n\t\t\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\t\t\t#note[\u0027services\u0027] \u003d service_list\n\t\t\t\trow_struct[\u0027value\u0027] \u003d referral \n\t\t\t\trow_struct[\u0027notes\u0027] \u003d note_list\n\t\t\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\t\t#Append the individual row date to the table data object\n\t\t\t\ttable_data.append(row_struct)\n\t\t\n\t\treturn table_data\n\t\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants ReferralsTableDetail view.custom.table_data\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "onChange": {
        "enabled": null,
        "script": "\t#system.perspective.print(\"Table Data Changed\")\n\tsystem.perspective.sendMessage(messageType\u003d\"refreshReferralDetail\", payload\u003d{}, scope\u003d\"session\")"
      }
    },
