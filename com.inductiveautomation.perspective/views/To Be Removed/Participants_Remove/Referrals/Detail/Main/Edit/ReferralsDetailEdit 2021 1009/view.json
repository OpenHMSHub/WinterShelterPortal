      "onChange": {
        "enabled": null,
        "script": "\timport traceback\n\ttry:\n\t\tif currentValue.value:\n\t\t\tsystem.perspective.print(\u0027Add Comment\u0027)\n\t\t\tnull \u003d None\n\t\t\t#Get the exisiting instances\n\t\t\tinstances \u003d self.getChild(\"root\").getChild(\"FlexRepeater\").props.instances\n\t\t\tif instances !\u003d None:\n\t\t\t\trow_num \u003d len(instances)\n\t\t\telse:\n\t\t\t\trow_num \u003d 0\n\t\t\t\tinstances \u003d []\n\t\t\t#system.perspective.print(str(row_num) + \u0027 rows found\u0027)\n\t\t\tobject \u003d {\n\t\t \t \"instanceStyle\": {\n\t\t    \t\"classes\": \"\"\n\t\t  \t\t},\n\t\t  \t\t\"instancePosition\": {},\n\t\t  \t\t\"time_created\": system.date.toMillis(system.date.now()),\n\t\t  \t\t\"type_id\": -1,\n\t\t  \t\t\"type_name\": \u0027\u0027,\n\t\t  \t\t\"status_id\": -1,\n\t\t  \t\t\"status_name\": \u0027\u0027,\n\t\t  \t\t\"comment\": \"\",\n\t\t  \t\t\"row_num\": row_num\n\t\t\t\t}\n\t\t\t#append to the list\n\t\t\tinstances.append(object)\n  \t\t\tself.getChild(\"root\").getChild(\"FlexRepeater\").props.instances \u003d instances\n\t\t\t#Reset the script trigger\n\t\t\t#system.perspective.print(str(len(instances)) + \u0027 rows\u0027)\n\t\tself.custom.func_add_comment \u003d False\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants ReferralDetailEdit view.custom.func_add_comment\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)\t"
      },
      "persistent": true
    },
