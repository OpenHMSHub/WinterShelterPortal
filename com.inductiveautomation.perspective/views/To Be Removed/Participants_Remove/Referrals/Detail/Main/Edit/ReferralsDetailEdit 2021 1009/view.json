                "onActionPerformed": {
                  "config": {
                    "script": "\timport traceback\n\ttry:\n\t\tproject \u003d \"RITI\"\n\t\t### Declare parameters \n\t\treferral_id \u003d self.view.custom.edit_data.referral_id\n\t\tuser_name \u003d self.session.props.auth.user.userName\n\t\t\n\t\tparticipant_id \u003d self.view.custom.edit_data.participant_id\n\t\tprovider_id \u003d self.view.custom.edit_data.provider_id\n\t\t#status_id \u003d self.view.custom.edit_data.status_id\n\t\ttype_id \u003d self.view.custom.edit_data.type_id\n\t\tif type_id \u003d\u003d 2:\n\t\t\tprogram_id \u003d None\n\t\telse:\n\t\t\tprogram_id \u003d self.view.custom.edit_data.program_id\n\t\t#note_log \u003d (self.view.custom.edit_data.notes)\n\t\t## Check if parameters are filled in correctly \n\t\tvalid \u003d True\n\t\t#if note_log is None and referral_id !\u003d -1:\n\t\t#\tvalid \u003d False\n\t\t#\terror_message \u003d \"At least one referral comment is required\"\n\t\t#elif len(note_log) \u003c 1 and referral_id !\u003d -1:\n\t\t#\tvalid \u003d False\n\t\t#\terror_message \u003d \"At least one referral comment is required\"\n\t\tif (referral_id is None):\n\t\t\tvalid \u003d False\n\t\t\terror_message \u003d \"Referral ID Error\"\n\t\telif (participant_id is None) or (int(participant_id) \u003c 0):\n\t\t\tvalid \u003d False\n\t\t\terror_message \u003d \"Participant ID Error\"\n\t\telif (provider_id is None) or (int(provider_id) \u003c 0):\n\t\t\tvalid \u003d False\n\t\t\terror_message \u003d \"Please select a Provider\"\n\t\telif (type_id is None) or (int(type_id) \u003c 0):\n\t\t\tvalid \u003d False\n\t\t\terror_message \u003d \"Referral Type Error\"\n\t\telse:\n\t\t\tvalid \u003d True\n\t\t\t#system.perspective.print(\"Referral Data Valid\")\n\t\t#Any referrals other than quick require a program\n\t\tif type_id !\u003d 2:\n\t\t\tif (program_id is None) or (int(type_id) \u003c 0):\n\t\t\t\tvalid \u003d False\n\t\t\t\terror_message \u003d \"Please select a Program\"\n\t\t\n\t\t\n\t\tif valid:\n\t\t\tif referral_id \u003d\u003d -1:\n\t\t\t\t#If new referral then check if quick or full\n\t\t\t\t#If full then status is 1 - Received\n\t\t\t\t#If quick then status is 5 - Approved\n\t\t\t\ttime_created \u003d system.date.now()\n\t\t\t\tif type_id \u003d\u003d 2:\n\t\t\t\t\tstatus_id \u003d 5\n\t\t\t\telse:\n\t\t\t\t\tstatus_id \u003d 12\n\t\t\telse:\n\t\t\t\t#If existing referral\n\t\t\t\t#Get the status_id of the last added note (name is blank)\n\t\t\t\tnote_date \u003d 0\n\t\t\t\tfor row in range(len(note_log)):\n\t\t\t\t\t#system.perspective.print(str(type(note_log[row].time_created)))\n\t\t\t\t\tdatestring \u003d str(type(note_log[row].time_created))\n\t\t\t\t\tif \u0027Date\u0027 in datestring:\n\t\t\t\t\t\trow_date \u003d system.date.toMillis(note_log[row].time_created)\n\t\t\t\t\telse:\n\t\t\t\t\t\trow_date \u003d note_log[row].time_created\n\t\t\t\t\t#system.perspective.print(row_date)\n\t\t\t\t\tif row_date \u003e note_date:\n\t\t\t\t\t\tlatest_event_row \u003d note_log[row].row_num\n\t\t\t\t\t\tnote_date \u003d row_date \n\t\t\t\tstatus_id \u003d note_log[latest_event_row].status_id\n\t\t\t\ttime_created \u003d note_log[latest_event_row].time_created\n\t\t\t\tif (status_id is None) or (int(status_id) \u003c 0):\n\t\t\t\t\tvalid \u003d False\n\t\t\t\t\terror_message \u003d \"Please select a Referral Status\"\n\t\t### Check if data is valid otherwise show error message.\n\t\tif valid:\t\t\n\t\t\t### Check if the user is creating new data\n\t\t\tif referral_id \u003d\u003d -1:#Insert new data, no row_id\n\t\t\t\t#system.perspective.print(\"Insert New Data\")\n\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailInsert\"\n\t\t\t\tparameters \u003d {\"participant_id\":participant_id,\n\t\t\t\t\t\t\t\"provider_id\":provider_id, \n\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\"type_id\":type_id,\n\t\t\t\t\t\t\t\"program_id\":program_id}\n\t\t\t\treferral_row \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t#Insert the comments\n\t\t\t\t#Quick referral gets a default comment\n\t\t\t\tif type_id \u003d\u003d 2:\n\t\t\t\t\tnote \u003d 0\n\t\t\t\t\tstatus_id \u003d 5\n\t\t\t\t\ttime_created \u003d system.date.now()\n\t\t\t\t\tcomment \u003d \"Quick referral created by \" + str(user_name)\n\t\t\t\t\treferral_id \u003d referral_row\n\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailNoteInsert\"\n\t\t\t\t\tparameters \u003d {\"comment\":comment,\n\t\t\t\t\t\t\t\t\"referral_id\":referral_id,\n\t\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\t\"type_id\":type_id,\n\t\t\t\t\t\t\t\t\"time_created\":time_created,\n\t\t\t\t\t\t\t\t\"user_name\":user_name}\n\t\t\t\t\tRefresh \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t#Full referral gets default comment\n\t\t\t\telse:\n\t\t\t\t\tnote \u003d 0\n\t\t\t\t\tstatus_id \u003d 12\n\t\t\t\t\ttime_created \u003d system.date.now()\n\t\t\t\t\tcomment \u003d \"Full referral created by \" + str(user_name)\n\t\t\t\t\treferral_id \u003d referral_row\n\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailNoteInsert\"\n\t\t\t\t\tparameters \u003d {\"comment\":comment,\n\t\t\t\t\t\t\t\t\"referral_id\":referral_id,\n\t\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\t\"type_id\":type_id,\n\t\t\t\t\t\t\t\t\"time_created\":time_created,\n\t\t\t\t\t\t\t\t\"user_name\":user_name}\n\t\t\t\t\tRefresh \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\n\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\n\t\t\t\n\t\t\t##### Check if the user is editing existing data\n\t\t\telif referral_id \u003e\u003d 0:#Update existing data on specified row_id\n\t\t\t\t#system.perspective.print(\"Update Exisiting Data\")\n\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailUpdate\"\n\t\t\t\tparameters \u003d {\"participant_id\":participant_id,\n\t\t\t\t\t\t\t\"provider_id\":provider_id, \n\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\"type_id\":type_id,\n\t\t\t\t\t\t\t\"program_id\":program_id,\n\t\t\t\t\t\t\t\"row_id\":referral_id}\n\t\t\t\tnothing \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t#\n\t\t\t\t#Auto add program and to waitlist on approval\n\t\t\t\tif status_id \u003d\u003d 5 and type_id !\u003d 2:\n\t\t\t\t\t#Enroll in the approved program\n\t\t\t\t\tquery_insert_name \u003d \"Participants/Enrollments/ParticipantAddSelectedEnrollments\"\n\t\t\t\t\tquery_insert_params \u003d {\"participantId\": participant_id, \"programId\": program_id}\n\t\t\t\t\tsystem.db.runNamedQuery(query_insert_name, query_insert_params)\n\t\t\t\t\t#Get the programs that have a waitlist\n\t\t\t\t\tpath \u003d \"Shelter/WaitList/WaitListProgramIDSelect\"\n\t\t\t\t\tprogram_list \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t\t#system.perspective.print(program_list)\n\t\t\t\t\tfor program in range(program_list.getRowCount()):\n\t\t\t\t\t\tif program_id \u003d\u003d program_list.getValueAt(program,0):\n\t\t\t\t\t\t\t#system.perspective.print(\u0027Check for waitlist entry\u0027)\n\t\t\t\t\t\t\t#Get the cross reference between the program ID and the waitlist program ID\n\t\t\t\t\t\t\tpath \u003d \"Shelter/WaitList/WaitListIDbyProgramSelect\"\n\t\t\t\t\t\t\tparameter \u003d {\"program_id\":program_id}\n\t\t\t\t\t\t\twaitlist_program_id \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t\t\t\t#waitlist_program_id \u003d waitlist_program_id_dataset.getValueAt(0,0)\n\t\t\t\t\t\t\t#system.perspective.print(\u0027Checking waitlist program \u0027 + str(waitlist_program_id))\n\t\t\t\t\t\t\t#Verify not already on the waitlist\n\t\t\t\t\t\t\tpath \u003d \"Shelter/WaitList/WaitListParticipantCheck\"\n\t\t\t\t\t\t\tparameters \u003d {\"participant_id\":participant_id, \"waitlist_program_id\":waitlist_program_id}\n\t\t\t\t\t\t\twaitlist_check \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t\t\t\t#system.perspective.print(\u0027Waitlist Rows \u003d \u0027 + str(waitlist_check.getRowCount()))\n\t\t\t\t\t\t\tif waitlist_check.getRowCount() \u003d\u003d 0:\n\t\t\t\t\t\t\t\t#system.perspective.print(\u0027Add to waitlist\u0027)\n\t\t\t\t\t\t\t\twaitlist_note \u003d \u0027Auto added to the waitlist on referral approval\u0027\n\t\t\t\t\t\t\t\tpath \u003d \"Shelter/WaitList/AddToWaitList\"\n\t\t\t\t\t\t\t\tparameters \u003d {\"participantid\":participant_id,\n\t\t\t\t\t\t\t\t\t\t\t\"programid\":waitlist_program_id,\n\t\t\t\t\t\t\t\t\t\t\t\"dateadded\":time_created,\n\t\t\t\t\t\t\t\t\t\t\t\"notes\":waitlist_note}\n\t\t\t\t\t\t\t\twaitlist_row \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\t\t\t\t\t#system.perspective.print(\u0027Added to waitlist\u0027)\t\t\n\t\t\t\t#Query, delete, and re-insert all comments linked to the referral\n\t\t\t\tpath \u003d \"Participants/Referrals/ReferralNotesSelect\"\n\t\t\t\tparameters \u003d {\"referral_id\":referral_id}\n\t\t\t\tcomment_list \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\t\n\t\t\t\t#system.perspective.print(service_list.getRowCount())\n\t\t\t\tif comment_list.getRowCount():\n\t\t\t\t\tfor comment in range(comment_list.getRowCount()):\n\t\t\t\t\t\tcomment_id \u003d comment_list.getValueAt(comment,\u0027note_id\u0027)\n\t\t\t\t\t\t#Delete the comment\n\t\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralNotesDelete\"\n\t\t\t\t\t\tparameters \u003d {\"row_id\":comment_id}\n\t\t\t\t\t\t#system.perspective.print(\u0027Delete Service: \u0027 + str(service))\n\t\t\t\t\t\tRefresh \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\t\n\t\t\t\t\t#Insert the comments\n\t\t\t\t\tfor row in range(len(self.view.custom.edit_data.notes)):\n\t\t\t\t\t\tnote \u003d self.view.custom.edit_data.notes[row]\n\t\t\t\t\t\tstatus_id \u003d note.status_id\n\t\t\t\t\t\ttime_created \u003d note.time_created\n\t\t\t\t\t\tcomment \u003d note.comment\n\t\t\t\t\t\treferral_id \u003d referral_id\n\t\t\t\t\t\tpath \u003d \"Participants/Referrals/ReferralDetailNoteInsert\"\n\t\t\t\t\t\tparameters \u003d {\"comment\":comment,\n\t\t\t\t\t\t\t\t\t\"referral_id\":referral_id,\n\t\t\t\t\t\t\t\t\t\"status_id\":status_id,\n\t\t\t\t\t\t\t\t\t\"type_id\":type_id,\n\t\t\t\t\t\t\t\t\t\"time_created\":time_created,\n\t\t\t\t\t\t\t\t\t\"user_name\":user_name}\n\t\t\t\t\t\tRefresh \u003d system.db.runNamedQuery(project\u003dproject,path\u003dpath,parameters\u003dparameters, getKey\u003dTrue)\n\t\t\t\telse:\n\t\t\t\t\tRefresh \u003d True\n\n\t\t\t##### Refresh the table\n\t\t\tif Refresh:\n\t\t\t\t#system.perspective.print(\"Referral Query Success\")\n\t\t\t\tself.getSibling(\"Label\").showMessage(\"Referral Updated\")\t\n\t\t\t#system.perspective.sendMessage(messageType\u003d\"RefreshReferralsData\", payload\u003d{\u0027row_num\u0027:referral_id}, scope\u003d\"session\")\n\t\t\tmessageType \u003d \u0027RefreshReferralsData\u0027\n\t\t\tsystem.perspective.sendMessage(messageType, payload \u003d {\u0027row_num\u0027:-1})\n\n\t\telse:\n\t\t\tself.getSibling(\"Label\").showMessage(error_message)\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants ReferralDetailEdit button_save\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)\n\t\tself.getSibling(\"Label\").showMessage(\"Something went wrong while saving data. Please try again.\")\t"
                  },
