              "transforms": [
                {
                  "code": "\t\"\"\"\n\t####THIS SECTION FOR INFORMATION ONLY####\n\t#Example Drug Screen Detail data object should look like this:\n\t{\n\t\t\"value\": {\n\t\t  \"test_date\": \"07/30/2020\",\n\t\t  \"administered_by\": \"Provider A\",\n\t\t  \"test_reason_id\":1\n\t\t  \"test_reason\": \"Court Request\",\n\t\t  \"test_type\": \"Hard Drug Screen\",\n\t\t  \"score\": \"100\",\n\t\t  \"grade_level\": \"A\"\n\t\t  \"passed\": true,\n\t\t  \"comments\": \"Good test!\",\n\t\t  \"id\": 0\n\t\t  \"employee_id\": 0\n\t\t  \"employee_name\":\"Willy Wonka\"\n\t\t  \"participant_id\": 0\n\t\t  \"provider_id\": 0\n\t\t  \"drug_screen_type_id\": 0\n\t\t  \"time_created\":\"07/30/2020\",\n\t\t \t }\n\t\t\"style\": {\n\t\t      \"classes\" \u003d \"Table/Cell_Dashboard\"\n\t\t\t  }\n\t\t}\n\t####END INFORMATION####\n\t\t\n\t####BEGIN ACTUAL SCRIPT####\n\t\"\"\"\n\t#table_data is the data object to return\n\ttable_data \u003d []\n\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\trow_struct \u003d {}\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\t\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027test_date\u0027] \u003d system.date.toMillis(value.getValueAt(row, \"test_date\"))\n\t\trow_data[\u0027time_created\u0027] \u003d system.date.toMillis(value.getValueAt(row, \"time_created\"))\n\t\trow_data[\u0027administered_by\u0027] \u003d value.getValueAt(row, \"administered_by\")\n\t\trow_data[\u0027test_reason_id\u0027] \u003d value.getValueAt(row, \"test_reason_id\")\t\n\t\trow_data[\u0027test_reason\u0027] \u003d value.getValueAt(row, \"test_reason\")\n\t\trow_data[\u0027test_type\u0027] \u003d value.getValueAt(row, \"test_type\")\n\t\trow_data[\u0027passed\u0027] \u003d value.getValueAt(row, \"passed\")\n\t\trow_data[\u0027comments\u0027] \u003d value.getValueAt(row, \"comments\")\n\t\trow_data[\u0027id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\trow_data[\u0027employee_id\u0027] \u003d value.getValueAt(row, \"employee_id\")\n\t\trow_data[\u0027employee_name\u0027] \u003d value.getValueAt(row, \"employee_name\")\n\t\trow_data[\u0027participant_id\u0027] \u003d value.getValueAt(row, \"participant_id\")\n\t\trow_data[\u0027provider_id\u0027] \u003d value.getValueAt(row, \"provider_id\")\n\t\trow_data[\u0027drug_screen_type_id\u0027] \u003d value.getValueAt(row, \"drug_screen_type_id\")\n\t\t#Set the default style for the row\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t#Append the individual row date to the table data object\n\t\ttable_data.append(row_struct)\n\t\t\t\n\treturn table_data\t",
                  "type": "script"
                }
              ],
