{
  "custom": {},
  "params": {
    "area": true,
    "astrix": true,
    "inputText": "",
    "placeholder": " ",
    "text": "Event Title"
  },
  "propConfig": {
    "params.area": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.astrix": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.inputText": {
      "binding": {
        "config": {
          "bidirectional": true,
          "path": "/root/TextArea.props.text"
        },
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.placeholder": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50,
      "width": 150
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "65px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.text"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Text/Add_HeaderText",
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Label_0"
            },
            "position": {
              "basis": "9px"
            },
            "propConfig": {
              "meta.visible": {
                "binding": {
                  "config": {
                    "path": "view.params.astrix"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "color": "#FF0000"
              },
              "text": "*"
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "basis": "-12px"
        },
        "props": {
          "style": {
            "marginBottom": 5,
            "overflow": "visible"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "TextArea"
        },
        "position": {
          "basis": "50px"
        },
        "propConfig": {
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.placeholder"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_TextArea_With_Border",
            "marginLeft": 5,
            "overflow": "hidden"
          },
          "wrap": "hard"
        },
        "type": "ia.input.text-area"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignContent": "flex-start",
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}