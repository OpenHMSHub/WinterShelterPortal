          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\timport traceback\n#\tTODO: \n#\t\t- Move to transaction\n#\t\t- Show error message\n\ttry:\n#\tGet the parameters from the input boxes\n\t\tname \u003d self.parent.parent.getChild(\"FlexContainer Personal\").getChild(\"FlexContainer Left\").getChild(\"FirstName\").props.params.inputText\n\t\tdescription \u003d self.parent.parent.getChild(\"FlexContainer Personal\").getChild(\"FlexContainer Left\").getChild(\"LastName\").props.params.inputText\n\t\t\n\t\tleaderId \u003d self.parent.parent.getChild(\"FlexContainer SelectedLeader\").getChild(\"FlexContainer Right\").getChild(\"LeaderVolId\").props.params.inputText\n\t\t\n\t\tif name !\u003d \u0027\u0027:\n\t\t\tif not description:\n\t\t\t\tdescription \u003d \"\"\n\t\t\tgroupParams \u003d {\"name\": name, \"description\": description}\n\t\t\tgroupId \u003d system.db.runNamedQuery(\"Volunteers/Group/createGroup\", groupParams)\n\t\t\tif leaderId ! \u003d\u003d \"\":\n\t\t\t\tleaderParams \u003d {\"groupId\": groupId, \"volunteerId\": leaderId}\n\t\t\t\tsystem.db.runNamedQuery(\"Volunteers/Group/UpdateGroupLeader\", leaderParams)\n\t\t\tsystem.perspective.closeDock(\"RegisterGroup\")\n\t\telse:\n\t\t\tself.getSibling(\"Label\").showMessage(\"Group Name is a required field to add a new Group.\")\n\t\tsystem.perspective.sendMessage(\"refreshGroupsTable\", scope\u003d\"session\")\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Groups GroupRegistration Button Create\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
