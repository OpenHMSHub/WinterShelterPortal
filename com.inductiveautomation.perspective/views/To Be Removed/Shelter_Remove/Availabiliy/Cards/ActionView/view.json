          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\troot \u003d self.parent\n\tlbl \u003d self.getSibling(\"Emv_AboutLabel\")\n\t\n\tdateIn \u003d root.getAsClientTime(system.date.now())\n\t\n\tlbl.props.params.Info \u003d \u0027\u0027\n\t\n\tpayload \u003d {\u0027dateIn\u0027: dateIn}\n\ttry:\n\t\tsystem.perspective.sendMessage(\u0027mhSetAvailabilityData\u0027, payload) #, scope \u003d \u0027page\u0027)\n\t\t\n\t\tlbl.props.params.Info \u003d system.date.format(dateIn,\"MMM d, yyyy\")\n\t\tlbl.props.params.Description \u003d \u0027Date\u0027 \n\texcept:\n\t\tlbl.props.params.Info \u003d \u0027error\u0027"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "btnView"
        },
