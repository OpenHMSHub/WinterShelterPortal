{
  "custom": {},
  "params": {
    "date": "Feb 28,2021 12:00 AM"
  },
  "propConfig": {
    "params.date": {
      "binding": {
        "config": {
          "path": "/root/Label.props.text"
        },
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 150
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "draggable": true,
                "id": "WOhAID5i",
                "modal": true,
                "overlayDismiss": true,
                "resizable": false,
                "showCloseIcon": false,
                "title": "Length of Reservation",
                "type": "open",
                "viewPath": "Shelter/Reservation/Cards/New Reservation/EndDateCalendar"
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "{session.custom.dateRangeToDate}"
              },
              "transforms": [
                {
                  "formatType": "datetime",
                  "formatValue": "MMM dd,yyyy hh:mm a",
                  "type": "format"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "classes": "DateTime/Add_Date",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "type": "ia.container.flex"
  }
}