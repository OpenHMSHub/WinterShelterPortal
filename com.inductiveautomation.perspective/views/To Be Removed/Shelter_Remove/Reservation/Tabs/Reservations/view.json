{
  "custom": {},
  "events": {
    "system": {
      "onStartup": {
        "config": {
          "script": "\tsystem.perspective.print(\"Reservations view - facility id is \")\n\tsystem.perspective.print(self.params.facilityId)\n#\tsystem.perspective.sendMessage(messageType\u003d\"refreshReservations\", payload\u003d{}, scope\u003d\"session\")"
        },
        "scope": "G",
        "type": "script"
      }
    }
  },
  "params": {
    "facilityId": 1
  },
  "propConfig": {
    "params.facilityId": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 1436
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView_3"
        },
        "position": {
          "basis": "290px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.facilityID": {
            "binding": {
              "config": {
                "path": "view.params.facilityId"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "reservationId": -1
          },
          "path": "Shelter/Reservation/Cards/Reservation_New",
          "style": {
            "classes": "ActivityCards/Card"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.facilityId": {
            "binding": {
              "config": {
                "path": "view.params.facilityId"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "active": null,
            "age": -1,
            "bed": null,
            "employeeId": -1,
            "expired": -1,
            "gender": -1,
            "race": -1,
            "room": -1,
            "shelter": -1,
            "waitinglist": -1
          },
          "path": "Shelter/Reservation/Filter/TableFilter"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_2"
        },
        "position": {
          "basis": "95%",
          "grow": 1
        },
        "propConfig": {
          "props.params.facilityID": {
            "binding": {
              "config": {
                "path": "view.params.facilityId"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "fetch": null,
            "resultRowCount": 1
          },
          "path": "Shelter/Reservation/Table/Reservations_Table",
          "style": {
            "marginLeft": 40,
            "marginRight": 40
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Alternative"
      }
    },
    "type": "ia.container.flex"
  }
}