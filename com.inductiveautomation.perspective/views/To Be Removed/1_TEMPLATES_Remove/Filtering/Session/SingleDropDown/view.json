{
  "custom": {},
  "params": {
    "column_name": "Gender",
    "filter": "",
    "key": "",
    "options": [],
    "placeholder": "Gender",
    "searchType": "match"
  },
  "propConfig": {
    "params.column_name": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    },
    "params.filter": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    },
    "params.key": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    },
    "params.options": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    },
    "params.placeholder": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    },
    "params.searchType": {
      "access": "PUBLIC",
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 309
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "filter"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.options"
              },
              "type": "property"
            }
          },
          "props.placeholder.text": {
            "binding": {
              "config": {
                "path": "view.params.placeholder"
              },
              "type": "property"
            }
          },
          "props.style.classes": {
            "binding": {
              "config": {
                "path": "view.params.selection"
              },
              "transforms": [
                {
                  "code": "\ttext \u003d \"Filter/Filter_Text_Default\"\n\tif value !\u003d None:\n\t\tif value !\u003d \"\" and value \u003e -1:\n\t\t\ttext \u003d \"Filter/Filter_Text_TeelBlue\"\n\treturn text",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "path": "session.custom.filters"
              },
              "transforms": [
                {
                  "code": "\tnewValue \u003d None\n\ttry:\n\t\tnewValue \u003d value[self.view.params.filter][self.view.params.key][\"value\"]\n\texcept:\n\t\tpass\n\t\t\n\treturn newValue",
                  "type": "script"
                }
              ],
              "type": "property"
            },
            "onChange": {
              "enabled": null,
              "script": "\t# Update the session\u0027s filter listing for this page\n\tif origin in [\"Browser\", \"Script\"]:\n\t\tfilter \u003d self.view.params.filter\t\n\t\tkey \u003d self.view.params.key\n\t\tcolumnName \u003d self.view.params.column_name\n\t\tsearchType \u003d self.view.params.searchType\n\t\tselection \u003d currentValue.value\n\t\t\t\n\t\tfilters.updateFilterList(\n\t\t\tself.session,\n\t\t\tfilter,\n\t\t\tsearchType,\n\t\t\tkey,\n\t\t\tcolumnName,\n\t\t\tselection\n\t\t)\t"
            }
          }
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Dropdowns/DD_Options"
          },
          "placeholder": {
            "color": "#AAAAAA"
          },
          "showClearIcon": true,
          "style": {},
          "wrapMultiSelectValues": false
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "Filter"
    },
    "propConfig": {
      "props.style.classes": {
        "binding": {
          "config": {
            "path": "./filter.props.value"
          },
          "transforms": [
            {
              "code": "\tborder \u003d \"Filter/Filter_Border_Default\"\n\tif value !\u003d None:\n\t\tborder \u003d \"Filter/Filter_Border_TeelBlue\"\n\treturn border",
              "type": "script"
            }
          ],
          "type": "property"
        }
      }
    },
    "props": {
      "direction": "column",
      "style": {}
    },
    "scripts": {
      "customMethods": [],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "clearFilter",
          "pageScope": true,
          "script": "\tself.getChild(\"filter\").props.value \u003d None",
          "sessionScope": false,
          "viewScope": true
        }
      ]
    },
    "type": "ia.container.flex"
  }
}