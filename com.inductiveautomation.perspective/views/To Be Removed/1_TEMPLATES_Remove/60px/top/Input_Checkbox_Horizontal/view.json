{
  "custom": {},
  "params": {
    "checked": true,
    "text": "label"
  },
  "propConfig": {
    "params.checked": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 75,
      "width": 175
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "basis": "36px"
        },
        "propConfig": {
          "props.selected": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.checked"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "checkedIcon": {
            "color": {
              "enabled": "#3EC3D9"
            }
          },
          "text": "",
          "uncheckedIcon": {
            "color": {
              "enabled": "#3EC3D9"
            }
          }
        },
        "type": "ia.input.checkbox"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "131px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_HeaderText"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignItems": "center"
    },
    "type": "ia.container.flex"
  }
}