  "params": {
    "funcAddVolunteer": true,
    "humanId": 15126,
    "outputName": "Anthony  Alegria",
    "volunteerId": 1226
  },
  "propConfig": {
    "custom.funcSelectVolunteer": {
      "onChange": {
        "enabled": null,
        "script": "\tif currentValue.value:\n\t\t#system.perspective.print(currentValue)\n\t\tvalid \u003d True\n\t\t#Verify its gretaer then -1\n\t\tif self.custom.volunteerId \u003c 0:\n\t\t\tvalid \u003d False\n\t\t\tsystem.perspective.print(\u0027Volunteer ID not valid\u0027)\n\t\tif valid:\t\t\n\t\t\tself.session.custom.filters.volunteerSearch.results.humanId \u003d self.custom.humanId\n\t\t\tself.session.custom.filters.volunteerSearch.results.volunteerId \u003d self.custom.volunteerId\n\t\t\tself.session.custom.filters.volunteerSearch.results.volunteerSelected \u003d True\n\t\t\t#self.params.volunteerId \u003d self.custom.volunteerId\n\t\t\t#self.params.humanId \u003d self.custom.humanId\n\t\t\t#self.params.outputName \u003d self.custom.selectedName\n\t\t\t#self.params.funcAddVolunteer \u003d True\n\t\t#Reset the script trigger\n\t\tself.custom.funcSelectVolunteer \u003d False\n\t\tpopupId \u003d \u0027volunteerSearch\u0027\n\t\tsystem.perspective.closePopup(popupId)"
      },
      "persistent": true
    },
    "custom.humanId": {
      "binding": {
        "config": {
          "path": "/root/Table.props.selection.data"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value[0].humanId\n\telse:\n\t\treturn -1",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.selectedName": {
      "binding": {
        "config": {
          "path": "/root/Table.props.selection.data"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn str(value[0].firstName) + \" \" + str(value[0].middleName)+ \" \" + str(value[0].lastName)\n\telse:\n\t\treturn -1",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.volunteerId": {
      "binding": {
        "config": {
          "path": "/root/Table.props.selection.data"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value[0].volunteerId\n\telse:\n\t\treturn -1",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.funcAddVolunteer": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.humanId": {
      "paramDirection": "output"
    },
    "params.outputName": {
      "paramDirection": "output"
    },
    "params.volunteerId": {
      "paramDirection": "output"
    }
