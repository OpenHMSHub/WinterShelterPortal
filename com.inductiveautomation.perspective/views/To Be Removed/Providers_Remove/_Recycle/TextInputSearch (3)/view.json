{
  "custom": {},
  "params": {
    "actionPulse": 0,
    "clear": false,
    "textLabel": "Time",
    "textValue": ""
  },
  "propConfig": {
    "params.actionPulse": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.textLabel": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.textValue": {
      "access": "PUBLIC",
      "binding": {
        "config": {
          "bidirectional": true,
          "path": "/root/TextField.props.text"
        },
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "25px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.textLabel"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_HeaderText"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onKeyUp": {
              "config": {
                "script": "\tself.view.params.actionPulse \u003d not(self.view.params.actionPulse)\n"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "TextField"
        },
        "position": {
          "basis": "25px",
          "grow": 1
        },
        "props": {
          "deferUpdates": false,
          "style": {
            "classes": "Text/Add_TextField",
            "marginBottom": "0px",
            "marginLeft": "5px"
          }
        },
        "type": "ia.input.text-field"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}