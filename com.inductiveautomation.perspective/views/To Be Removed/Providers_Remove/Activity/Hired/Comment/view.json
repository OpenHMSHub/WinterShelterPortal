{
  "custom": {},
  "params": {
    "Text": "value"
  },
  "propConfig": {
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "100%",
          "grow": 1,
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "alignVertical": "top",
          "style": {
            "classes": "ActivityCards/Card_TextDark",
            "marginLeft": "50px",
            "marginRight": "50px",
            "overflow": "hidden",
            "paddingTop": "5px"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}