{
  "custom": {},
  "params": {
    "Filter": "Referrals",
    "label": "Summary"
  },
  "propConfig": {
    "params.Filter": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label_Summary"
        },
        "position": {
          "basis": "25px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.label"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Summary_Title"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}