{
  "custom": {},
  "params": {
    "employee_id": -1,
    "enrollment_id": 0,
    "row_num": -1,
    "service_id": 0
  },
  "propConfig": {
    "params.employee_id": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.enrollment_id": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.row_num": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.service_id": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 55,
      "width": 1100
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Embed_Employee"
        },
        "position": {
          "basis": "330px"
        },
        "propConfig": {
          "props.params.options": {
            "binding": {
              "config": {
                "queryPath": "Participants/EmployeeDropdownList"
              },
              "type": "query"
            }
          },
          "props.params.selection": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.employee_id"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "text": "User Performing the Service"
          },
          "path": "Participants/Templates/Input_DropDownSingle",
          "style": {
            "classes": "InputCards/InputCard_TopBorder"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Embed_Enrollment"
        },
        "position": {
          "basis": "440px",
          "grow": 1
        },
        "propConfig": {
          "props.params.options": {
            "binding": {
              "config": {
                "queryPath": "Participants/ProgramDropdownList"
              },
              "type": "query"
            }
          },
          "props.params.selection": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.enrollment_id"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "text": "Enrollment"
          },
          "path": "Participants/Templates/Input_DropDownSingle",
          "style": {
            "classes": "InputCards/InputCard_TopBorder"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Embed_Service"
        },
        "position": {
          "basis": "440px",
          "grow": 1
        },
        "propConfig": {
          "props.params.options": {
            "binding": {
              "config": {
                "parameters": {
                  "program_id": "{view.params.enrollment_id}"
                },
                "queryPath": "Participants/ServiceDropdownList"
              },
              "type": "query"
            }
          },
          "props.params.selection": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.service_id"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "text": "Service"
          },
          "path": "Participants/Templates/Input_DropDownSingle",
          "style": {
            "classes": "InputCards/InputCard_TopBorder"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Embed_Remove"
        },
        "position": {
          "basis": "80px"
        },
        "propConfig": {
          "props.params.row_num": {
            "binding": {
              "config": {
                "path": "view.params.row_num"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Participants/Case Notes/Detail/Cards/Service_Remove",
          "style": {
            "classes": "InputCards/InputCard_TopBorder"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}