        "events": {
          "component": {
            "onRowClick": {
              "config": {
                "script": "\tselectedColumn \u003d self.props.selection.selectedColumn\n\t#Close the preview screen if open\n\tif selectedColumn !\u003d \"Options\":\n\t\tsystem.perspective.closeDock(\u0027PreviewShelter\u0027)\n\t\n\tif selectedColumn \u003d\u003d \"ShelterName\": \n\t\t#Display the detail screen\n\t\tshelterID \u003d self.props.selection.data[0][\u0027id\u0027]\n\n#\tAdded interim shelter param to test Room Layout by Ron Lund 11-1-2020\t\n\t\tsystem.tag.write(\u0027[default]RITI/Facilities/ShelterFacID\u0027,shelterID)\n#\tEnd Ron Lund code snip\n\n\t\tsystem.perspective.navigate(view \u003d \u0027Shelter/Detail/Shelter_Detail\u0027, params \u003d {\u0027shelterID\u0027:shelterID})\n\telif selectedColumn \u003d\u003d \"Options\":\n\t\tshelterID \u003d self.props.selection.data[0][\u0027id\u0027]\n\t\tid \u003d \"PreviewShelter\" \n\t\tparams \u003d {\"shelterID\":shelterID}\n\t\tsystem.perspective.openDock(id\u003did,params\u003dparams)\n\telse:\n\t\tpass\n\t"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "100%",
          "grow": 1
        },
        "propConfig": {
          "custom.filteredData": {
            "binding": {
              "config": {
                "struct": {
                  "filters": "{this.custom.filters}",
                  "source": "hasChanged({this.custom.sqlData})"
                },
                "waitOnAll": true
              },
              "transforms": [
                {
                  "code": "\treturn filters.applyFiltersOnDataset(self.custom.sqlData, self.custom.filters)",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
            }
          },
          "custom.filters": {
            "binding": {
              "config": {
                "path": "session.custom.filters"
              },
              "transforms": [
                {
                  "code": "\treturn filters.retrievePageFilters(value, self.page.props.pageId)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "custom.sqlData": {
            "binding": {
              "config": {
                "queryPath": "Shelter/Dashboard/ShelterDashboardQuery"
              },
              "type": "query"
            }
          },
          "props.data": {
            "binding": {
              "config": {
                "path": "this.custom.filteredData"
              },
              "transforms": [
                {
                  "code": "\t\n\tshelter \u003d []\n\tshelterNameStyle \u003d {}\n\tshelterNameStyle[\u0027color\u0027] \u003d \u0027#0c4e85\u0027\n\tshelterNameStyle[\u0027cursor\u0027] \u003d \"pointer\"\n\tshelterNameStyle[\u0027fontFamily\u0027] \u003d \"Proxima Nova ExCn Medium\"\n\tshelterNameStyle[\u0027text-align\u0027] \u003d \"left\"\n\tfor row in range(value.getRowCount()):\n\t\tiRow \u003d {key: value.getValueAt(row,key) for key in value.columnNames}\n\t\tif filters.applyFilters(iRow,self.custom.filters):\n\t\t\toneShelter \u003d {}\n\t\t\tshelterName \u003d {}\n\t\t## add elemts from dataset\n\t\t\toneShelter[\u0027id\u0027] \u003d str(value.getValueAt(row, \"id\"))\n\t\t\n\t\t\tshelterName[\u0027style\u0027] \u003d shelterNameStyle\n\t\t\tshelterName[\u0027value\u0027] \u003d str(value.getValueAt(row, \"ShelterName\"))\n\t\t\toneShelter[\u0027ShelterName\u0027] \u003d shelterName\n\t\t\n\t\t\toneShelter[\u0027ShelterType\u0027] \u003d str(value.getValueAt(row, \"ShelterType\"))\n\t\t\toneShelter[\u0027address\u0027] \u003d str(value.getValueAt(row, \"address\"))\n\t\t\toneShelter[\u0027zipCode\u0027] \u003d str(value.getValueAt(row, \"zipCode\"))\n\t\t\toneShelter[\u0027totalBeds\u0027] \u003d str(value.getValueAt(row, \"beds\"))\n\t\t\toneShelter[\u0027phone\u0027] \u003d str(value.getValueAt(row, \"phone\"))\n\t\t\toneShelter[\u0027noOfReservations\u0027] \u003d str(value.getValueAt(row, \"noOfReservations\"))\n\t\t\toneShelter[\u0027associatedProgram\u0027] \u003d str(value.getValueAt(row, \"associatedProgram\"))\n\t\t\toneShelter[\u0027WaitingList\u0027] \u003d str(value.getValueAt(row, \"WaitingList\"))\n\t\t\toneShelter[\u0027noOfCheckins\u0027] \u003d str(value.getValueAt(row, \"noOfCheckins\"))\n\t\t\n\t\t\toneShelter[\u0027availableBeds\u0027] \u003d int(oneShelter[\u0027totalBeds\u0027]) - int(oneShelter[\u0027noOfReservations\u0027]) - int(oneShelter[\u0027noOfCheckins\u0027])\n\t\t\toneShelter[\u0027Options\u0027] \u003d \u0027\u0027\n\t\t\n\t\t\tshelter.append(oneShelter)\n\treturn shelter\n\t",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
