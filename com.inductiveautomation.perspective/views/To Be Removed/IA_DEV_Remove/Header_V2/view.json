{
  "custom": {},
  "params": {
    "Text": "Tyler George Adams"
  },
  "propConfig": {
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 130,
      "width": 287
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Image"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "fit": {
                "mode": "contain"
              },
              "source": "/system/images/Images/Tyler_headshot.png"
            },
            "type": "ia.display.image"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "basis": "100px"
        },
        "props": {
          "justify": "center"
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "16px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Personal_Header",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}