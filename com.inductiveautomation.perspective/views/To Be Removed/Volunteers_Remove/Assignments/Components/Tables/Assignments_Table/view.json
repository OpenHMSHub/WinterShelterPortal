          "custom.sqlData": {
            "binding": {
              "config": {
                "queryPath": "Volunteers/Assignments/AssignmentDashboard"
              },
              "type": "query"
            }
          },
          "props.data": {
            "binding": {
              "config": {
                "path": "this.custom.filteredData"
              },
              "transforms": [
                {
                  "code": "\t####THIS SECTION FOR INFORMATION ONLY####\n\t#Example Volunteer data object should look like this:\n\t{\n\t\t  \"id\": 1,\n\t\t  \"Next Assignment\": \"Next Assignment\",\n\t\t  \"LastServiceDate\": \"4/4/2019\",\n\t\t  \"Last Assignment\": \"Last Assignment\",\n\t\t  \"Name\": {\n\t\t    \"style\": {\n\t\t      \"color\": \"#0c4e85\",\n\t\t      \"cursor\": \"pointer\",\n\t\t      \"fontFamily\": \"Proxima Nova ExCn Medium\\n\",\n\t\t      \"text-align\": \"left\"\n\t\t    },\n\t\t    \"value\": \"Tyler George Adams\"\n\t\t  },\n\t\t  \"Options\": \"\",\n\n\t\t}\n\t####END INFORMATION####\n\t\n\t####BEGIN ACTUAL SCRIPT####\n\t#Volunteers is the data object to return\n\tvolunteers \u003d []\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\t#v is an individual volunteer to appends to \u0027volunteers\u0027\n\t\tv \u003d {}\n\t\t#n is the \u0027name\u0027 attribute of \u0027v\u0027\n\t\tn \u003d {}\n\t\t#s is the \u0027style\u0027 attribute of \u0027n\u0027\n\t\ts \u003d {}\n\t\t\n\t\t#Map query results into correct attributes\n\t\t#v[\u0027NextServiceDate\u0027] \u003d value.getValueAt(row, \"Next Service Date\")\n\t\tdays \u003d value.getValueAt(row, \"days\") if value.getValueAt(row, \"days\") else 0\n\t\tv[\u0027days\u0027] \u003d assignments.daysToStr(days)\n\t\tv[\u0027id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\tv[\u0027timeStart\u0027] \u003d assignments.minutesToTimeStr(value.getValueAt(row, \"timeStart\"))\n\t\tv[\u0027converageToday\u0027] \u003d value.getValueAt(row, \"converageToday\") if value.getValueAt(row, \"converageToday\") is not None else \"0/0\"\n\t\tv[\u0027converageTomorrow\u0027] \u003d value.getValueAt(row, \"converageTomorrow\") if value.getValueAt(row, \"converageTomorrow\") is not None else \"0/0\"\n\t\tv[\u0027converageSevenDay\u0027] \u003d value.getValueAt(row, \"converageSevenDay\") if value.getValueAt(row, \"converageSevenDay\") is not None else \"0/0\"\n\t\t\n\t\t\n\t\t#Style is static\n\t\ts[\u0027color\u0027] \u003d \u0027#0c4e85\u0027\n\t\ts[\u0027cursor\u0027] \u003d \"pointer\"\n\t\ts[\u0027fontFamily\u0027] \u003d \"Proxima Nova ExCn Medium\"\n\t\ts[\u0027text-align\u0027] \u003d \"left\"\n\t\t\n\t\t#Build the \u0027name\u0027 attribute\n\t\tn[\u0027style\u0027] \u003d s\n\t\tn[\u0027value\u0027] \u003d str(value.getValueAt(row, \"volunteerJobName\"))\n\t\tv[\u0027Name\u0027] \u003d n\n\t\t\n\t\tv[\u0027Options\u0027] \u003d \u0027\u0027\n\t\t\n\t\t\n\t\t#Append the individual volunteer to the volunteerss object\n\t\tvolunteers.append(v)\n\t\t\n\treturn volunteers",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
