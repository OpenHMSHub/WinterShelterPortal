        }
      },
      "custom._devPulse": {
        "onChange": {
          "enabled": null,
          "script": "\t\n\temvRace \u003d self.getChild(\"emvRace\")\n\temvYear \u003d self.getChild(\"emvYear\")\n\temvSearch \u003d self.getChild(\"emvSearch\")\n\t\n\tif currentValue.value is True:\n\t\tself.setPropsDropdowns()\n\telse:\n\t\temvRace.props.params.options \u003d []\n\t\temvYear.props.params.options \u003d []\n\t\n\temvRace.props.params.actionPerformedPulse \u003d False\n\temvYear.props.params.actionPerformedPulse \u003d False\n\temvSearch.props.params.keyUpPulse \u003d False"
