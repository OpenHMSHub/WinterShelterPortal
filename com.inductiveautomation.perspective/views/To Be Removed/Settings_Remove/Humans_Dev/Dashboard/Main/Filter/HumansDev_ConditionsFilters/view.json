            "onActionPerformed": {
              "config": {
                "script": "\troot \u003d self.parent\n\t\n\temvFirstName \u003d root.getChild(\"emvFirstName\") \n\temvMiddleName \u003d root.getChild(\"emvMiddleName\")\n\temvLastName \u003d root.getChild(\"emvLastName\")\n\temvNickName \u003d root.getChild(\"emvNickName\")\n\temvSsn \u003d root.getChild(\"emvSsn\")\n\temvRace \u003d root.getChild(\"emvRace\")\n\temvYear \u003d root.getChild(\"emvYear\")\n\temvSearch \u003d root.getChild(\"emvSearch\")\n\n\t\n\temvYear.props.params.clear \u003d True\n\temvRace.props.params.clear \u003d True\n\temvSearch.props.params.clear \u003d True\n\temvFirstName.props.params.clear \u003d True\n\temvMiddleName.props.params.clear \u003d True\n\temvLastName.props.params.clear \u003d True\n\temvNickName.props.params.clear \u003d True\n\temvSsn.props.params.clear \u003d True\t\n\t\t\n\tpayload \u003d {}\n\tpayload[\u0027year\u0027] \u003d None\n\tpayload[\u0027raceIds\u0027] \u003d None\n\t\n\t_dict \u003d {}\n\t_dict[\u0027first_name\u0027] \u003d None\n\t_dict[\u0027last_name\u0027] \u003d None\n\t_dict[\u0027middle_name\u0027] \u003d None\n\t_dict[\u0027nick_name\u0027] \u003d None\n\t_dict[\u0027ssn\u0027] \u003d None\n\tpayload[\u0027searchByColumns\u0027] \u003d None #_dict\n\n\tpayload[\u0027searchText\u0027] \u003d None\n\n\troot.sendMsgConditionAndFilter(payload)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
