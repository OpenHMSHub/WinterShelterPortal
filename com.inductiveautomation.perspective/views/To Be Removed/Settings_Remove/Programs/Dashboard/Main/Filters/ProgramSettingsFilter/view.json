{
  "custom": {},
  "params": {
    "filter_active": 1
  },
  "propConfig": {
    "params.filter_active": {
      "binding": {
        "config": {
          "path": "/root/Active.props.value"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 1470
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Active"
        },
        "position": {
          "basis": "140px"
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Table/Filter_Header_Dropdown"
          },
          "options": [
            {
              "label": "All Programs",
              "value": -1
            },
            {
              "label": "Active Only",
              "value": 1
            }
          ],
          "style": {
            "classes": "Table/Filter_Header_Text"
          },
          "value": 1
        },
        "type": "ia.input.dropdown"
      },
      {
        "meta": {
          "name": "Placeholder"
        },
        "position": {
          "basis": "1px",
          "grow": 1
        },
        "props": {
          "path": "Settings/Templates/Placeholder"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Search"
        },
        "position": {
          "basis": "205px",
          "shrink": 0
        },
        "props": {
          "path": "Participants/Dashboard/Components/Buttons/Search_Bar",
          "style": {
            "marginBottom": "5px",
            "marginRight": "40px",
            "marginTop": "5px"
          },
          "useDefaultViewHeight": true
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignContent": "center",
      "alignItems": "center",
      "justify": "flex-end",
      "style": {
        "borderStyle": "none",
        "classes": "Table/Filter_Header_Container",
        "overflow": "hidden"
      },
      "wrap": "wrap"
    },
    "type": "ia.container.flex"
  }
}