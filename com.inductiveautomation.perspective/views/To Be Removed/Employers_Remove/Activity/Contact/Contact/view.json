{
  "custom": {
    "Data": [
      {
        "email": "",
        "name": "",
        "phone": ""
      }
    ]
  },
  "params": {
    "Date": 0,
    "ID": 5,
    "Size": 110
  },
  "propConfig": {
    "custom.Data": {
      "binding": {
        "config": {
          "parameters": {
            "contactID": "{view.params.ID}"
          },
          "queryPath": "Employers/Activities/SelectSingleContact",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value\n\telse:\n\t\treturn [{\n\t  \"name\": \"\",\n\t  \"phone\": \"\",\n\t  \"email\": \"\"\n\t}]",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.Date": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "binding": {
        "config": {
          "path": "view.props.defaultSize.height"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 110
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Date": {
            "binding": {
              "config": {
                "expression": "{view.params.Date}"
              },
              "transforms": [
                {
                  "expression": "dateFormat({value},\"MMM. d, YYYY\") +\" at \"+ dateFormat({value},\"hh:mm a\")",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Icon": "material/description",
            "Title": "Contact"
          },
          "path": "Employers/Activity/ActivityHeader"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_1"
        },
        "position": {
          "basis": "64px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.email": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].email"
              },
              "type": "property"
            }
          },
          "props.params.name": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].name"
              },
              "type": "property"
            }
          },
          "props.params.phone": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].phone"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Employers/Activity/Contact/InformationTop",
          "style": {
            "borderBottomColor": "#e6e6e6",
            "borderBottomStyle": "none",
            "borderBottomWidth": 1,
            "borderTopColor": "#e6e6e6",
            "borderTopStyle": "solid",
            "borderTopWidth": 1,
            "marginLeft": "50px",
            "marginRight": "50px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}