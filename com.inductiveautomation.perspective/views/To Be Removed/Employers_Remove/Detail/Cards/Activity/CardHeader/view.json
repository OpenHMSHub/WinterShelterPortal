{
  "custom": {},
  "params": {
    "Title": "Participant created"
  },
  "propConfig": {
    "params.Title": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "100%",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "alignVertical": "bottom",
          "style": {
            "classes": "ActivityCards/Card_Title",
            "paddingBottom": "5px"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "ActivityCards/Header_Card"
      }
    },
    "type": "ia.container.flex"
  }
}