{
  "custom": {},
  "params": {
    "Text": "",
    "trackID": null
  },
  "propConfig": {
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.trackID": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 170,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "draggable": true,
                "id": "Ud90O6FO",
                "modal": true,
                "overlayDismiss": false,
                "position": {
                  "relativeLocation": "center"
                },
                "positionType": "relative",
                "resizable": true,
                "showCloseIcon": true,
                "title": "Upload Image",
                "type": "open",
                "viewParams": {
                  "trackID": "{view.params.trackID}"
                },
                "viewPath": "Employers/Detail/UploadImage"
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "Image"
        },
        "position": {
          "basis": "120px",
          "shrink": 0
        },
        "propConfig": {
          "props.altText": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "transforms": [
                {
                  "code": "\t\n\treturn value[0] if value else \u0027 \u0027",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.trackID"
              },
              "transforms": [
                {
                  "code": "\t\n\treturn \"/system/images/employers/{}.jpg\".format(value)\n",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "fit": {
            "height": 120,
            "mode": "absolute",
            "width": 120
          },
          "style": {
            "classes": "Rounded_Image",
            "cursor": "pointer",
            "marginLeft": "auto",
            "marginRight": "auto",
            "marginTop": "10px"
          }
        },
        "type": "ia.display.image"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Personal_Header",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "borderBottomColor": "#E6E6E6",
        "borderBottomStyle": "solid",
        "borderBottomWidth": "1px",
        "classes": "Text/Personal_Header",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}