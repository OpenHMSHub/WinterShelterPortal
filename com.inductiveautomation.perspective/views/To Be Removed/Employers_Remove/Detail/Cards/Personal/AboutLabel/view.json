{
  "custom": {},
  "params": {
    "Description": "Employer name",
    "Info": "Three Sons Mover"
  },
  "propConfig": {
    "params.Description": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Info": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 45,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "basis": "26px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Description"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/About_description"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Info"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "alignVertical": "top",
          "style": {
            "classes": "Text/About_info",
            "overflowWrap": "break-word",
            "paddingRight": "6px",
            "textOverflow": "ellipsis"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Text/Personal_Header",
        "overflow": "visible"
      }
    },
    "type": "ia.container.flex"
  }
}