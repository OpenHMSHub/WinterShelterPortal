{
  "custom": {},
  "params": {
    "Title": "\"Title\"",
    "Value": 100000
  },
  "propConfig": {
    "params.Title": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 140,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Value"
        },
        "position": {
          "basis": "50%",
          "grow": 1,
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "numberFormat({view.params.Value}, \u0027###,###\u0027)\r\n"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "alignVertical": "bottom",
          "style": {
            "classes": "KPIs/KPI_Value",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Title"
        },
        "position": {
          "basis": "20%",
          "grow": 1,
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "alignVertical": "top",
          "style": {
            "classes": "KPIs/KPI_Title",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "backgroundColor": "#FFFFFF",
        "paddingLeft": 5
      }
    },
    "type": "ia.container.flex"
  }
}