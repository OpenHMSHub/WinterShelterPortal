{
  "custom": {},
  "params": {
    "options": [],
    "selection": null
  },
  "propConfig": {
    "params.options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selection": {
      "paramDirection": "inout"
    }
  },
  "props": {
    "defaultSize": {
      "height": 75,
      "width": 175
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
          "basis": "50px"
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.options"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.selection"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Text/Add_InfoText"
          },
          "placeholder": {
            "icon": {
              "color": "#0d80e3"
            },
            "text": "Select"
          },
          "style": {
            "classes": "Text/Add_InfoText"
          }
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}