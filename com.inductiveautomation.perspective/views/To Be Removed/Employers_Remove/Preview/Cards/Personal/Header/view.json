{
  "custom": {},
  "params": {
    "Text": "Tyler George Adams",
    "trackID": 1
  },
  "propConfig": {
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.trackID": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 105,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Image"
        },
        "position": {
          "basis": "65px",
          "shrink": 0
        },
        "propConfig": {
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.trackID"
              },
              "transforms": [
                {
                  "code": "\treturn \"/system/images/employers/{}.jpg\".format(value)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "fit": {
            "height": 65,
            "mode": "absolute",
            "width": 65
          },
          "style": {
            "classes": "Rounded_Image",
            "marginLeft": "auto",
            "marginRight": "auto",
            "marginTop": "10px"
          }
        },
        "type": "ia.display.image"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Personal_Header",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Text/Personal_Header",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}