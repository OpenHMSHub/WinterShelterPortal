{
  "custom": {},
  "params": {},
  "props": {},
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Tabstrip"
            },
            "position": {
              "basis": "320px",
              "grow": 1
            },
            "props": {
              "params": {
                "selectedTab": "Overview"
              },
              "path": "Employers/Dashboard/Components/TabStrip/TabStrip"
            },
            "type": "ia.display.view"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "EmbeddedView_2"
                },
                "position": {
                  "basis": "320px"
                },
                "props": {
                  "params": {
                    "Text": "NEW EMPLOYER"
                  },
                  "path": "Employers/Dashboard/Components/Buttons/New_Employer",
                  "style": {
                    "paddingLeft": 4
                  },
                  "useDefaultViewHeight": true
                },
                "type": "ia.display.view"
              }
            ],
            "meta": {
              "name": "Controls"
            },
            "position": {
              "basis": "150px"
            },
            "props": {
              "style": {
                "classes": "Background/Default",
                "marginRight": "40px"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "Tabs_Header"
        },
        "position": {
          "basis": "55px",
          "shrink": 0
        },
        "props": {
          "justify": "space-between",
          "style": {
            "classes": "Background/Default",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Content"
        },
        "position": {
          "basis": "320px",
          "grow": 1
        },
        "propConfig": {
          "props.path": {
            "binding": {
              "config": {
                "path": "../Tabs_Header/Tabstrip.props.params.selectedTab"
              },
              "transforms": [
                {
                  "fallback": null,
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": "Overview",
                      "output": "Employers/Dashboard/Components/Tabs/Dashboard_Employers"
                    },
                    {
                      "input": "Applicants",
                      "output": "Employers/Applicants/Dashboard/Main/ApplicantsDashboard"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
