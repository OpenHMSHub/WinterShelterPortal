{
  "custom": {
    "dateFormatted": ""
  },
  "params": {
    "date": "",
    "text": "Time"
  },
  "propConfig": {
    "custom.dateFormatted": {
      "persistent": true
    },
    "params.date": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 34,
      "width": 150
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "DateTimeInput"
        },
        "position": {
          "basis": "35px"
        },
        "propConfig": {
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.date"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "format": "MMMM D, YYYY",
          "formattedValue": "January 4, 2021",
          "formattedValues": {
            "date": "Jun 23, 2020",
            "datetime": "Jun 23, 2020 10:49 AM",
            "time": "10:49 AM"
          },
          "inputProps": {
            "style": {
              "cursor": "pointer"
            }
          },
          "modalStyle": {
            "classes": "DateTime/Add_Date"
          },
          "pickerType": "date",
          "style": {
            "classes": "DateTime/Add_Date"
          }
        },
        "type": "ia.input.date-time-input"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}